

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <script src="https://kit.fontawesome.com/a84a069617.js" crossorigin="anonymous"></script>
    <link href="/modules/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    
    <link href="~/css/StyleSheet.css" rel="stylesheet" />
    
    <title>ETrade Company</title>
</head>
<body>
    

    <partial name="_navbar" />

    <div class="container mt-4">
        @if (TempData["message"] != null)
        {
            <partial name="_resultmessage" model="@(TempData.Get<ResultMessage>("message"))" />
        }
        @RenderBody()
    </div>
    <script src="/modules/jquery/dist/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
    <script src="/modules/bootstrap/dist/js/bootstrap.min.js"></script>
    @RenderSection("Scripts", false)
</body>
</html>
